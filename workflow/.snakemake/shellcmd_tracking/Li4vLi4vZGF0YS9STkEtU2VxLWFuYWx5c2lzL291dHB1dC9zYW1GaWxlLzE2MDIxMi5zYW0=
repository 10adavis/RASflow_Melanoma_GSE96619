hisat2 -p 10 -x ../../data/RNA-Seq-analysis/output/indexes/index -1 ../../data/RNA-Seq-analysis/output/reads/160212_forward.fastq.gz -2 ../../data/RNA-Seq-analysis/output/reads/160212_reverse.fastq.gz -S ../../data/RNA-Seq-analysis/output/samFile/160212.sam && samtools view -b -S ../../data/RNA-Seq-analysis/output/samFile/160212.sam > ../../data/RNA-Seq-analysis/output/bamFile/160212.bam